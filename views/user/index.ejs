		<script>
			let mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
			let usernameRegex = /^[a-zA-Z0-9]+$/;
			let passwwordregx = /^(?=.*\p{Ll})(?=.*\p{Lu})(?=.*[\d|@#$!%*?&])[\p{L}\d@#$!%*?&]{6,36}$/gu
			let phoneNumberregx = /^(\+91[\-\s]?)?[0]?(91)?[56789]\d{9}$/
			let resetPasswordOtp = ""
			let userMail = ""


			let reUsername = ""
			let reFullname = ""
			let rePhoneNumber = ""
			let reEmail = ""
			let reFlatNo = ""
			let reStreet = ""
			let reLandmark = ""
			let rePincode = ""
			let reDistrict = ""
			let rePassword = ""
			let generatedOTP = ""
			let otpInputField = ""

			function showError(htmlElement, errorMsg) {
				document.getElementById(htmlElement).innerHTML = `<div class="alert alert-danger" role="alert">${errorMsg}</div>`
				setTimeout(() => {
					document.getElementById(htmlElement).innerHTML = `<div></div>`
				}, 6000)
			}

			function signin() {
				const loginUsername = document.getElementById("SignInUsername").value;
				const loginPassword = document.getElementById("SignInPassword").value;

				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify({
						username: loginUsername,
						password: loginPassword,
					})
				}

				fetch("/postSignin", requestOptions)
					.then((response) => {
						if (!response.ok) {
							response.json().then((error) => {
								showError('SignInErrMsg', error.message);
							})
						}
						else {
							response.json().then(data => {
								location.replace(`/`);
							})
						}
					})
					.then((data) => {
						console.log(data, "Data");
					})
					.catch((error) => {
						console.log(error, "error");
					});
			};

			function signup() {
				let signUpUsername = document.getElementById("signUpUsername").value;
				let signUpFullname = document.getElementById("signUpFullname").value;
				let SignUpPhoneNumber = document.getElementById("SignUpPhoneNumber").value;
				let signUpEmail = document.getElementById("signUpEmail").value;
				let signUpFlatNo = document.getElementById("signUpFlatNo").value;
				let signUpStreet = document.getElementById("signUpStreet").value;
				let signUpLandmark = document.getElementById("signUpLandmark").value;
				let signUpDistrict = document.getElementById("signUpDistrict").value;
				let signUpPincode = document.getElementById("signUpPincode").value;
				let signUpPassword = document.getElementById("signUpPassword").value;
				let signUpConfirmPassword = document.getElementById("signUpConfirmPassword").value;

				if (
					signUpUsername == "" ||
					signUpFullname == "" ||
					SignUpPhoneNumber == "" ||
					signUpEmail == "" ||
					signUpFlatNo == "" ||
					signUpStreet == "" ||
					signUpLandmark == "" ||
					signUpDistrict == "" ||
					signUpPincode == "" ||
					signUpPassword == ""
				) {
					showError('errMsg', "Make sure you have entered all fields");
					return false;
				} else if (!signUpEmail.match(mailformat)) {
					showError('errMsg', "You have entered an invalid email address");
					return false;
				} else if (!signUpUsername.match(usernameRegex)) {
					showError('errMsg',
						"Your username is not valid. Only characters A-Z, a-z and '-', numbers are  acceptable"
					);
					return false;
				} else if (signUpPassword.length < 5) {
					showError('errMsg',
						"Make sure your password contains atleast 5 charecters or numbers"
					);
					return false;
				} else if (signUpPassword.length > 16) {
					showError('errMsg',
						"Your password is too long make sure it is less than 16 charecters or numbers"
					);
					return false;
				} else if (!signUpPassword.match(passwwordregx)) {
					showError("errMsg", "The password must be at least 6 characters long and must contain at least one lowercase letter, one uppercase letter, one digit, and one special character (@, #, $, !, %, *, ?, &)")
					return false;
				} else if (!SignUpPhoneNumber.match(phoneNumberregx)) {
					showError("errMsg", "Phone number not valid")
					return false;
				} else if (signUpPassword !== signUpConfirmPassword) {
					showError('errMsg', "Password doesn't match");
					return false;
				}

				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify({
						username: signUpUsername,
						fullname: signUpFullname,
						phoneNumber: SignUpPhoneNumber,
						email: signUpEmail,
						flatNo: signUpFlatNo,
						street: signUpStreet,
						landmark: signUpLandmark,
						district: signUpDistrict,
						pincode: signUpPincode,
						password: signUpPassword,
					})
				}

				fetch("/postSendOtp", requestOptions)
					.then((response) => response.json())
					.then((data) => {
						if (data.status == 200) {
							console.log(data)
							console.log("data.username", data.data.username)
							reUsername = data.data.username
							reFullname = data.data.fullname
							rePhoneNumber = data.data.phoneNumber
							reEmail = data.data.email
							reFlatNo = data.data.flatNo
							reStreet = data.data.street
							reLandmark = data.data.landmark
							rePincode = data.data.pincode
							reDistrict = data.data.district
							rePassword = data.data.password
							generatedOTP = data.data.generatedOTP
							$(".js-panel-otp").addClass("show-header-myaccount");
							$(".js-panel-editInfo").removeClass("show-header-myaccount");
							$(".js-panel-otp").addClass("js-hide-editInfo js-show-otp");

							$(".js-hide-otp").on("click", function () {
								$(".js-panel-otp").removeClass("show-header-myaccount");
								location.replace('/');
							});
						} else {
							console.log("else err")
							showError('errMsg', data.errorMsg);
						}
					})
					.catch((error) => {
						console.log(error, "catch error");
						showError('errMsg', error.message);
					});
			}

			function sendotp() {
				const otpInputField = document.getElementById("otpInputField").value;
				const reqbody = {
					username: reUsername,
					fullname: reFullname,
					phoneNumber: rePhoneNumber,
					email: reEmail,
					flatNo: reFlatNo,
					street: reStreet,
					landmark: reLandmark,
					district: reDistrict,
					pincode: rePincode,
					password: rePassword,
					generatedOTP: generatedOTP,
					otpInputField: otpInputField
				};
				console.log(reqbody, 'reqbody')
				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify(reqbody)
				}

				fetch("/postSignup", requestOptions)
					.then((response) => response.json())
					.then((data) => {
						if (data.status == 200) {
							location.replace(`/`);
						} else {
							showError('OTPErrMsg', "OTP Does not match");
						}
					})
					.catch((error) => {
						console.log(error, "error");
						showError('OTPErrMsg', "Something went wrong! Try again later");
					});
			}

			function forgotPassWordSendOtp() {
				const forgotEmail = document.getElementById("forgotEmail").value;
				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify({
						email: forgotEmail
					})
				}
				fetch("/postForgotPassword", requestOptions)
					.then((response) => {
						if (!response.ok) {
							response.json().then((error) => {
								showError('sendOtpErr', error.message);
							})
						}
						else {
							response.json().then(data => {
								$(".js-panel-resetPasswordStep2").addClass("show-header-myaccount");
								$(".js-panel-editInfo").removeClass("show-header-myaccount");
								$(".js-panel-resetPassword").removeClass("show-header-myaccount");
								$(".js-panel-myaccount").removeClass("show-header-myaccount");
								$(".js-panel-resetPasswordStep2").addClass("js-hide-editInfo js-hide-otp js-hide-myaccount js-hide-resetPassword js-show-resetPasswordStep2");
								resetPasswordOtp = data.body.forgotPasswordOTP
								userMail = data.body.userMail
							})
						}
					})
					.then()
					.catch((error) => {
						console.log(error);
					});
			}

			function resetPassword() {
				const confirmOTP = document.getElementById("confirmOTP").value;
				const newPassword = document.getElementById("newPassword").value;
				const confirmNewPassword = document.getElementById("confirmNewPassword").value;

				if (confirmOTP == "") {
					showError('resetPasswordError', "OTP is missing");
					return false;
				} else if (!newPassword.match(passwwordregx)) {
					showError('resetPasswordError', "The password must be at least 6 characters long and must contain at least one lowercase letter, one uppercase letter, one digit, and one special character (@, #, $, !, %, *, ?, &)");
					return false;
				} else if (newPassword !== confirmNewPassword) {
					showError('resetPasswordError', "Password and confirm password should be same");
					return false;
				}
				console.log(resetPasswordOtp, "resetPasswordOtp")
				console.log(userMail, "userMail");
				console.log(confirmOTP, "otpFromUser");
				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify({
						newPassword: newPassword,
						otpFromUser: confirmOTP,
						otpFromBackend: resetPasswordOtp,
						email: userMail
					})
				}

				fetch("/postResetForgotPassword", requestOptions)
					.then((response) => {
						if (!response.ok) {
							response.json().then((error) => {
								showError('resetPasswordError', error.message);
							})
						}
						else {
							response.json().then(data => {
								location.replace(`/`);
							})
						}
					})
					.then((data) => response.json())
					.catch((error) => {
						console.log(error);
					});
			}

			let userBlockSwal = "<%=body.userBlockSwal%>"
			if (userBlockSwal) {
				console.log(userBlockSwal, " INSIDE user blcok");
				window.addEventListener("load", () => {
					swal({
						title: "User blocked",
						text: "Contact administrator for more details",
						icon: "warning",
						buttons: true,
						dangerMode: true,
						buttons: "Cancel",

					})
				})
			}
		</script>
