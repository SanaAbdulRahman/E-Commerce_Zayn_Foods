<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>zAYn Foods The Best Food Delivery App In Calicut</title>

	<link rel="icon" type="image/png" href="images/icons/favicon.png" />

	<link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">

	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

	<link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">

	<link rel="stylesheet" type="text/css" href="fonts/iconic/css/material-design-iconic-font.min.css">

	<link rel="stylesheet" type="text/css" href="fonts/linearicons-v1.0.0/icon-font.min.css">

	<link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">

	<link rel="stylesheet" type="text/css" href="vendor/css-hamburgers/hamburgers.min.css">

	<link rel="stylesheet" type="text/css" href="vendor/animsition/css/animsition.min.css">

	<link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">

	<link rel="stylesheet" type="text/css" href="vendor/daterangepicker/daterangepicker.css">

	<link rel="stylesheet" type="text/css" href="vendor/slick/slick.css">

	<link rel="stylesheet" type="text/css" href="vendor/MagnificPopup/magnific-popup.css">

	<link rel="stylesheet" type="text/css" href="vendor/perfect-scrollbar/perfect-scrollbar.css">

	<link rel="stylesheet" type="text/css" href="stylesheets/util.css">
	<link rel="stylesheet" type="text/css" href="stylesheets/main.css">
	<link rel="stylesheet" href="stylesheets/style.css">


</head>

<body class="animsition">
	<!-- Header -->
	<header>
		<!-- Header desktop -->
		<div class="container-menu-desktop">
			<!-- Topbar -->
			<div class="top-bar">
				<div class="content-topbar flex-sb-m h-full container">
					<div class="left-top-bar">
						Enjoy Free shipping for orders above ₹499!
					</div>
				</div>
			</div>

			<div class="wrap-menu-desktop">
				<nav class="limiter-menu-desktop container">

					<!-- Logo desktop -->
					<a href="#" class="logo">
						<img src="images/icons/logo-01.png" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
							<li class="active-menu">
								<a href="">Home</a>
							</li>
							<li>
								<a href="/about">About</a>
							</li>
							<li>
								<a href="/contact">Contact</a>
							</li>
						</ul>
					</div>

					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">
						<button
							class="flex-c-m stext-101 cl0 size-104 bg1 bor1 hov-btn1 p-lr-15 trans-04 js-show-myaccount">
							Sign In
						</button>
					</div>
				</nav>
			</div>
		</div>

		<!-- Header Mobile -->
		<div class="wrap-header-mobile">
			<!-- Logo moblie -->
			<div class="logo-mobile">
				<a href="/index"><img src="images/icons/logo-01.png" alt="IMG-LOGO"></a>
			</div>

			<!-- Icon header -->
			<div class="wrap-icon-header flex-w flex-r-m m-r-15">

				<a href="#" class="cl1 mtext-102 hov-cl3 flex-c-m trans-04 p-lr-25 js-show-myaccount">
					Sign In
				</a>
			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			<ul class="topbar-mobile">
				<li>
					<div class="left-top-bar">
						Free shipping for orders above ₹499
					</div>
				</li>
			</ul>

			<ul class="main-menu-m">
				<li class="active-menu">
					<a href="">Home</a>
				</li>
				<li>
					<a href="/dishes">Dishes</a>
				</li>

				<li>
					<a href="/about">About</a>
				</li>

				<li>
					<a href="/contact">Contact</a>
				</li>
			</ul>
		</div>

	</header>


	<!-- Sign In-->
	<div class="wrap-header-myaccount js-panel-myaccount">
		<div class="s-full js-hide-cart"></div>

		<div class="header-myaccount flex-col-l p-l-65 p-r-25">
			<div class="header-myaccount-title flex-w flex-sb-m p-b-8">
				<span class="mtext-103 cl2">
					Sign In
				</span>

				<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-myaccount">
					<i class="zmdi zmdi-close"></i>
				</div>
			</div>

			<div class="header-myaccount-content flex-w js-pscroll">
				<ul class="header-myaccount-wrapitem w-full">
					<form id="signInForm">
						<span class="mtext-102 cl2">
							Username
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="username"
								placeholder="eg : Jhon007" id="SignInUsername">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Password
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="password" name="password"
								placeholder="********" id="SignInPassword">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div id="SignInErrMsg" class="w-full ">

						</div>
						<a href="#" class="js-show-resetPassword"><span class="stext-102 cl8">
								Forgot password?
							</span><span> Reset Password</span></a>
						<div class="p-t-18">
							<button onclick="signin()"
								class="flex-c-m stext-101 cl0 size-117 bg1 bor1 hov-btn1 p-lr-15 trans-04"
								type="button">
								Sign In
							</button>
						</div>
					</form>
				</ul>

				<div class="w-full">
					<a href="#" class="js-show-editInfo"><span class="stext-102 cl8">
							Don't have an account?
						</span> <span> Sign Up</span></a>
				</div>
			</div>
		</div>
	</div>

	<!-- OTP-->
	<div class="wrap-header-myaccount js-panel-otp">
		<div class="s-full js-hide-otp"></div>

		<div class="header-myaccount flex-col-l p-l-65 p-r-25">
			<div class="header-myaccount-title flex-w flex-sb-m p-b-8">
				<span class="mtext-103 cl2">
					Verify OTP
				</span>

				<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-otp">
					<i class="zmdi zmdi-close"></i>
				</div>
			</div>
			<div class="header-myaccount-content flex-w js-pscroll">
				<ul class="header-myaccount-wrapitem w-full">
					<form>
						<span class="mtext-102 cl2">
							OTP
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="otp" id="otpInputField"
								placeholder="Enter OTP here">
							<div class="focus-input1 trans-04"></div>
						</div>
						<a href="#"><span class="stext-102 cl8">
								OTP Has been sent into your mail<br>
							</span><span id="otpTimer"></span></a>
						<input type="text" name="username" id="reUsername" value="<%= body.username %>"
							style="display: none;">
						<input type="text" name="fullname" id="reFullname" value="<%= body.fullname %>"
							style="display: none;">
						<input type="text" name="phoneNumber" id="rePhoneNumber" value="<%= body.phoneNumber %>"
							style="display: none;">
						<input type="text" name="email" id="reEmail" value="<%= body.email %>" style="display: none;">
						<input type="text" name="password" id="rePassword" value="<%= body.password %>"
							style="display: none;">
						<input type="text" name="flatNo" id="reFlatNo" value="<%= body.flatNo %>"
							style="display: none;">
						<input type="text" name="street" id="reStreet" value="<%= body.street %>"
							style="display: none;">
						<input type="text" name="landmark" id="reLandmark" value="<%= body.landmark %>"
							style="display: none;">
						<input type="text" name="pincode" id="rePincode" value="<%= body.pincode %>"
							style="display: none;">
						<input type="text" name="district" id="reDistrict" value="<%= body.district %>"
							style="display: none;">
						<input type="text" name="generatedOTP" id="" value="<%= body.generatedOTP %>"
							style="display: none">
						<div id="OTPErrMsg" class="w-full "></div>
						<div class="p-t-18">
							<button class="flex-c-m stext-101 cl0 size-117 bg1 bor1 hov-btn1 p-lr-15 trans-04"
								type="button" onclick="sendotp()">
								Submit
							</button>
						</div>
					</form>
				</ul>
			</div>
		</div>
	</div>


	<!-- Reset Password-->
	<div class="wrap-header-myaccount js-panel-resetPasswordStep2">
		<div class="s-full js-resetPasswordStep2-"></div>

		<div class="header-myaccount flex-col-l p-l-65 p-r-25">
			<div class="header-myaccount-title flex-w flex-sb-m p-b-8">
				<span class="mtext-103 cl2">
					Reset Password
				</span>
				<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-resetPasswordStep2">
					<i class="zmdi zmdi-close"></i>
				</div>
			</div>

			<div class="header-myaccount-content flex-w js-pscroll">
				<ul class="header-myaccount-wrapitem w-full">
					<form>
						<span class="mtext-102 cl2">
							OTP
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text"
								placeholder="Enter received OTP" id="confirmOTP">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							New Password
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="password" id="newPassword"
								placeholder="Enter a strong password">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="password" name="password"
								placeholder="Confirm your password" id="confirmNewPassword">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div id="resetPasswordError"></div>
						<div class="p-t-18">
							<button type="button"
								class="flex-c-m stext-101 cl0 size-117 bg1 bor1 hov-btn1 p-lr-15 trans-04"
								onclick="resetPassword()">
								Reset Password
							</button>
						</div>
					</form>
				</ul>
			</div>
		</div>
	</div>

	<!-- Forgot Password-->
	<div class="wrap-header-myaccount js-panel-resetPassword">
		<div class="s-full js-hide-resetPassword"></div>

		<div class="header-myaccount flex-col-l p-l-65 p-r-25">
			<div class="header-myaccount-title flex-w flex-sb-m p-b-8">
				<span class="mtext-103 cl2">
					Reset Password
				</span>

				<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-resetPassword">
					<i class="zmdi zmdi-close"></i>
				</div>
			</div>

			<div class="header-myaccount-content flex-w js-pscroll">
				<ul class="header-myaccount-wrapitem w-full">
					<form>
						<span class="mtext-102 cl2">
							Email
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="email" name="email" required
								id="forgotEmail" placeholder="Enter your email">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="w-full">
							<span class="stext-102">
								Click the "Send OTP" Button and check your email !
							</span>
						</div>
						<div id="sendOtpErr"></div>
						<div class="p-t-18">
							<button type="button"
								class="flex-c-m stext-101 cl0 size-117 bg1 bor1 hov-btn1 p-lr-15 trans-04"
								onclick="forgotPassWordSendOtp()">
								Send OTP
							</button>
						</div>
					</form>
				</ul>
			</div>
		</div>
	</div>

	<!-- SignUp-->
	<div class="wrap-header-myaccount js-panel-editInfo">
		<div class="s-full js-hide-cart"></div>

		<div class="header-myaccount flex-col-l p-l-65 p-r-25">
			<div class="header-myaccount-title flex-w flex-sb-m p-b-8">
				<span class="mtext-103 cl2">
					Sign Up
				</span>

				<div class="fs-35 lh-10 cl2 p-lr-5 pointer hov-cl1 trans-04 js-hide-editInfo">
					<i class="zmdi zmdi-close"></i>
				</div>
			</div>

			<div class="header-myaccount-content flex-w js-pscroll">
				<ul class="header-myaccount-wrapitem w-full">
					<form id="signUpform">
						<span class="mtext-102 cl2">
							Username
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="username"
								id="signUpUsername" placeholder="eg : Jhon007">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Full Name
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="fullname"
								id="signUpFullname" placeholder="eg : Jhon Abraham">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Phone Number
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="tel" name="phoneNumber"
								id="SignUpPhoneNumber" value="+91 ">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Email Id
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="email" id="signUpEmail"
								placeholder="email@example.com">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Address
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="flatNo" id="signUpFlatNo"
								placeholder="Flat Name, No">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="street" id="signUpStreet"
								placeholder="Street">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="landmark"
								id="signUpLandmark" placeholder="Landmark">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="text" name="district"
								id="signUpDistrict" placeholder="District">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="number" name="pincode"
								id="signUpPincode" placeholder="Pincode">
							<div class="focus-input1 trans-04"></div>
						</div>
						<span class="mtext-102 cl2">
							Password
						</span>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="password" name="password"
								id="signUpPassword" placeholder="Password">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div class="wrap-input1 w-full p-b-4 mb-4 mt-2">
							<input class="input1 bg-none plh1 stext-107 cl2" type="password" name="confirmPassword"
								id="signUpConfirmPassword" placeholder="Confirm Password">
							<div class="focus-input1 trans-04"></div>
						</div>
						<div id="errMsg" class="w-full ">
							<!-- validation code -->
						</div>
						<div class="p-t-18">
							<button type="button" onclick="signup()"
								class="flex-c-m stext-111 cl0 size-117 bg1 bor1 hov-btn1 p-lr-15 trans-04">
								Sign Up
							</button>
						</div>
					</form>
				</ul>
				<div class="w-full p-t-27">
					<a href="#" class="js-show-otp js-hide-editInfo"><span class="stext-102 cl8">
							Already have an account?
						</span> <span> Login</span></a>
				</div>
			</div>
		</div>
	</div>

	<!-- Slider -->
	<section class="section-slide">
		<div class="wrap-slick1">
			<div class="slick1">
				<% slider.forEach((slide)=>{ %>
					<div class="item-slick1" style="background-image: url(<%=slide.image%>);">
						<div class="container h-full">
							<div class="flex-col-l-m h-full p-t-100 p-b-30 respon5">
								<div class="layer-slick1 animated visible-false" data-appear="fadeInDown"
									data-delay="0">
									<span class="ltext-101 cl2 respon2">
										<%= slide.primaryText %>
									</span>
								</div>

								<div class="layer-slick1 animated visible-false" data-appear="fadeInUp"
									data-delay="800">
									<h2 class="ltext-203 cl2 p-t-19 p-b-43 respon1">
										<%= slide.altText %>
									</h2>
								</div>
							</div>
						</div>
					</div>
					<% }) %>
			</div>
		</div>
	</section>


	<!-- Banner -->
	<div class="sec-banner bg0 p-t-80 p-b-50">
		<div class="container">
			<div class="row">
				<% offerBox.forEach((box)=>{ %>
					<div class="col-md-6 col-xl-4 p-b-30 m-lr-auto">
						<!-- Block1 -->
						<div class="block1 wrap-pic-w">
							<img src="<%=box.image%>" alt="IMG-BANNER">

							<a href="<%=box.link ? box.link : " #" %>"
								class="block1-txt ab-t-l s-full flex-col-l-sb p-lr-38 p-tb-34 trans-03 respon3">
								<div class="block1-txt-child1 flex-col-l">
									<span class="block1-info stext-102-1 cl2 trans-04">
										<%=box.name%>
									</span>
								</div>

								<div class="block1-txt-child2 p-b-4 trans-05">
									<div class="block1-link ltext-102 cl0 trans-09">
										<%=box.price ? "₹" : "" %>
											<%=box.price ? box.price : "" %>
									</div>
								</div>
							</a>
						</div>
					</div>
					<% })%>
			</div>
		</div>
	</div>


	<!-- Product -->
	<section class="bg0 p-t-23 p-b-140">
		<div class="container">
			<div class="p-b-10">
				<h3 class="ltext-103 cl5">
					Dishes
				</h3>
			</div>

			<div class="flex-w flex-l-m filter-tope-group m-tb-10">
				<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1" data-filter="*">
					All Dishes
				</button>
				<% categories.slice(0,5).forEach((category)=> { %>
					<button class="stext-106 cl6 hov1 bor3 trans-04 m-r-32 m-tb-5 how-active1"
						data-filter=".<%=category.categoryName%>">
						<%= category.categoryName %>
					</button>
					<% }) %>
			</div>

			<!-- Product Card -->
			<div class="row isotope-grid">

				<% products.forEach((dish)=> { %>
					<% let filter=dish.productCategory.categoryName %>
						<div
							class="col-sm-6 col-md-4 col-lg-3 p-b-35 isotope-item <%=dish.productCategory.categoryName%>">
							<!-- Block2 -->
							<div class="block2">
								<div class="block2-pic hov-img0">
									<!-- product Image -->
									<img style="height: 250px; width:300px ;" src="<%= dish.productImage1 %>"
										alt="IMG-PRODUCT">

									<!-- Button -->
									<button type="button" onclick="detailView()"
										class="block2-btn flex-c-m stext-103 cl2 size-102 bg0 bor2 hov-btn1 p-lr-15 trans-04">
										Detailed View
									</button>
								</div>

								<div class="block2-txt flex-w flex-t p-t-14">
									<div class="block2-txt-child1 flex-col-l ">
										<a href="/dishSingleView"
											class="mtext-102 cl4 hov-cl1 trans-04 js-name-b2 p-b-6">
											<%= dish.productName %>
										</a>
										<div class="flex-w p-b-20">
											<div class="size-203">
												<span class="fs-18 cl11">
													<% if(!dish.ratingScore || dish.ratingScore <=0){ %>
														<i class="zmdi zmdi-star-outline"></i>
														<i class="zmdi zmdi-star-outline"></i>
														<i class="zmdi zmdi-star-outline"></i>
														<i class="zmdi zmdi-star-outline"></i>
