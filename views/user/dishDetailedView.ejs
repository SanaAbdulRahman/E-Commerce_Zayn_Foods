		<script>

			function showError(htmlElement, errorMsg) {
				document.getElementById(htmlElement).innerHTML = `<div class="alert alert-danger" role="alert">${errorMsg}</div>`
				setTimeout(() => {
					document.getElementById(htmlElement).innerHTML = `<div></div>`
				}, 6000)
			}


			const ratingForm = document.querySelector('.w-full');
			const ratingInput = document.querySelector('#rateingDetect');
			const reviewInput = document.querySelector('#review');
			const productId = "<%=product._id %>"

			ratingForm.addEventListener('submit', (event) => {
				event.preventDefault();

				const requestOptions = {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify({
						rating: ratingInput.value,
						message: reviewInput.value,
						productId: productId
					})
				}

				console.log(requestOptions.body)

				fetch("/postAddReview", requestOptions)
					.then((response) => {
						if (!response.ok) {
							response.json().then((error) => {
								showError('reviewErr', error.message);
							})
						}
						else {
							response.json().then(data => {
								location.reload(`/`)
							})
						}
					})
					.then()
					.catch((error) => {
						console.log(error);
					});


			});

		</script>

